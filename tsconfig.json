{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/features/*": [
        "features/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/assets/*": [
        "assets/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ]
}